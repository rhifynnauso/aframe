{
  "name": "@mozvr/gazebo",
  "version": "0.1.6",
  "homepage": "https://github.com/MozVR/vr-markup",
  "license": "MIT",
  "main": "src/vr-markup.js",
  "dependencies": {
    "document-register-element": "^0.4.5",
    "three-vr-browserify": "^1.0.2",
    "tween.js": "^15.0.0"
  },
  "devDependencies": {
    "browserify": "^11.0.1",
    "budo": "^4.2.1",
    "garnish": "^2.3.0",
    "karma": "^0.12.32",
    "karma-mocha": "^0.1.10",
    "karma-sinon-chai": "^0.3.0",
    "tween.js": "^15.0.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run build && budo src/vr-markup.js:build/vr-markup.js --debug --verbose --live --port 9000 -- -r ./src/vr-markup.js:vr-markup | garnish -v",
    "build": "mkdir -p build && browserify -r ./src/vr-markup.js:vr-markup -o build/vr-markup.js --debug",
    "test": "karma start"
  },
  "repository": "MozVR/vr-markup",
  "bugs": {
    "url": "https://github.com/MozVR/vr-markup/issues"
  }
